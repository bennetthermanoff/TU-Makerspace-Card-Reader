{
  "name": "makerspace-react",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm run start",
    "server": "cd server && nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "postinstall": "cd client && npm install",
    "serve":"cd client && npm run build && cd ../server && npm run serve"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.4.2",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "nodemon": "^2.0.16",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "concurrently": "^4.1.2"
  }
}
